#: ../../src/network/nethost.cc:1216
msgid "/help  -  Shows this help"
msgstr "/help  –  Показва това съобщение"

#. * TRANSLATORS: Available host command
#: ../../src/network/nethost.cc:366
msgid "/announce <msg>  -  Send a chatmessage as announcement (system chat)"
msgstr "/announce <съобщ.>  –  Изпраща чат съобщение като обявление (системен чат)"

#. * TRANSLATORS: Available host command
#: ../../src/network/nethost.cc:368
msgid "/warn <name> <reason>  -  Warn the user <name> because of <reason>"
msgstr "/warn <име> <причина>  –  Предупреждава потребителя <име> поради <причина>"

#. * TRANSLATORS: Available host command
#: ../../src/network/nethost.cc:370
msgid "/kick <name> <reason>  -  Kick the user <name> because of <reason>"
msgstr "/kick <име> <причина>  –  Изритва потребителя <име> поради <причина>"

