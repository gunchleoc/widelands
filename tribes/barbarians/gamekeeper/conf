[global]
help=_The Gamekeeper makes sure that the natural population of animals never drops too low.
program=release

[buildcost]
carrier=1

[release]
0=setbobdescription tribe:wildboar tribe:deer sheep
1=findspace size:any radius:3
2=walk coords
3=animation releasein 2000 # Play a releasing animation
4=create_bob
5=animation releaseout 2000
6=return

[releasein]
pics=releasein_??.png
hotspot=15 22
fps=5

[releaseout]
pics=releaseout_??.png
hotspot=15 22
fps=5

[idle]
pics=idle_??.png
hotspot=14 22

[walk]
dirpics=walk_!!_??.png
hotspot=15 22
fps=10
